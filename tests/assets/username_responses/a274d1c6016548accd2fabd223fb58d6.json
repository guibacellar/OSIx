                  borderWidth: 1,\n                        pointRadius: 2,\n                    }]\n                },\n                options: {\n                    animation: {\n                        duration: 200\n                    },\n                    layout: {\n                        padding: {\n                            left: 15,\n                            right: 15,\n                            top: 20,\n                            bottom: 5\n                        }\n                    },\n                    legend: {\n                        display: false\n                    },\n                    maintainAspectRatio: false,\n                    title: {\n                        display: false\n                    },\n                    tooltips: {\n                        mode: 'index',\n                        intersect: false,\n                        callbacks: {\n                            title: function (tooltipItems, data) {\n                                var tooltipItem = tooltipItems[0];\n                                var lp = tooltipItem.yLabel;\n\n                                var value = (lp - (lp % 100)) / 100;\n                                if (value >= 24) {\n                                    if (data.labels[tooltipItem.index].tier === 11) {\n                                        value = 26;\n                                    } else if (data.labels[tooltipItem.index].tier === 21) {\n                                        value = 25;\n                                    } else {\n                                        value = 24;\n                                    }\n                                }\n\n                                var factor = 26 - value;\n                                if (factor % 1 !== 0) {\n                                    factor -= (factor % 1);\n                                }\n\n                                return [\n                                    'Challenger 1'