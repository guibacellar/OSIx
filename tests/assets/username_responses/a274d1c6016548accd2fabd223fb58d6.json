             } else {\n                                    lp = lp % 100;\n                                }\n\n                                return ' ' + lp + ' LP';\n                            }\n                        }\n                    },\n                    scales: {\n                        xAxes: [{\n                            display: true,\n                            scaleLabel: {\n                                display: false\n                            },\n                            ticks: {\n                                fontColor: '#a0a0a0',\n                                callback: function (value, index, values) {\n                                    return value.date;\n                                }\n                            }\n                        }],\n                        yAxes: [{\n                            display: true,\n                            scaleLabel: {\n                                display: false\n                            },\n                            ticks: {\n                                fontColor: '#646464',\n                                suggestedMin: 3200,\n                                suggestedMax: 5200,\n                                callback: function (value, index, values) {\n                                    var factor = 26 - (value / 100);\n\n                                    if (value >= 3200) {\n                                        if (value > 3200) {\n                                            return null;\n                                        }\n\n                                        factor = 0;\n                                    } else if (value >= 2800) {\n                                        if (value > 2800) {\n                                            return null;\n                                        }\n\n                                        factor = 1;\n                                    } else if (value >= 2400) {\n                                        if (value > 2400) {\n                                            return null;\n                                        }\n\n                                        factor = 2;\n                                    }\n\n                                    if (factor % 1 !== 0) return null;\n\n                                    var label = [\n                                        'Challenger 1',\n                                        'Grandmaster 1',\n                                        'Master 1',\n                                        'Diamond 1',\n                                        'Diamond 2',\n                                        'Diamond 3',\n                                        'Diamond 4',\n                                        'Platinum 1',\n                                        'Platinum 2',\n                                        'Platinum 3',\n                                        'Platinum 4',\n                                        'Gold 1',\n                                        'Gold 2',\n                                        'Gold 3',\n                                        'Gold 4',\n                                        'Silver 1',\n                                        'Silver 2',\n                                        'Silver 3',\n                                        'Silver 4',\n                                        'Bronze 1',\n                                        'Bronze 2',\n                                        'Bronze 3',\n                                        'Bronze 4',\n                                        'Iron 1',\n                                        'Iron 2',\n                                        'Iron 3',\n                                        'Iron 4',\n                                    ][factor];\n\n                                    return label;\n                                }\n                            }\n                        }]\n        