n                        alert(response.message);\n\n                        if ($this.hasClass('active') === true) {\n                            $this.removeClass('active');\n                            $this.find('i').removeClass('fas').addClass('far');\n                        } else {\n                            $this.addClass('active');\n                            $this.find('i').removeClass('far').addClass('fas');\n                        }\n                    }\n                },\n                error: function (xhr, ajaxOptions, thrownError) {\n                    if (xhr.status < 1) return;\n\n                    if (xhr.status >= 500) {\n                        alert('Server error occurred.');\n                    } else {\n                        alert('Network error occurred.');\n                    }\n\n                    if ($this.hasClass('active') === true) {\n                        $this.removeClass('active');\n                        $this.find('i').removeClass('fas').addClass('far');\n                    } else {\n                        $this.addClass('active');\n                        $this.find('i').removeClass('far').addClass('fas');\n                    }\n                }\n            });\n\n            return false;\n        });\n\n        $('.profile__match-history-v2__item .func .toggle-detail').on('click', function () {\n            let $this = $(this);\n            let $match = $this.closest('.profile__match-history-v2__item');\n            let $detail = $match.next();\n\n            if ($detail.is(':visible') === false) {\n                $detail.show();\n                $this.find('i').removeClass('fa-chevron-down').addClass('fa-chevron-up');\n\n                if ($detail.data('load') !== true) {\n                    $detail.html('<div class=\"text-center py-5 position-relative\" style=\"background-color: #fafafa;\"><i class=\"fa fa-spinner fa-spin\" style=\"font-size: 30px;\"></i></div>');\n                    axios.get('h