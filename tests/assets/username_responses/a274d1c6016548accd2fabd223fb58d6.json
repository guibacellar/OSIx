   display: false\n                    },\n                    scales: {\n                        yAxes: [{\n                            ticks: {\n                                suggestedMin: 0,\n                                suggestedMax: chartData.sort().slice(-1)[0],\n                            }\n                        }]\n                    },\n                }\n            });\n        }\n\n\n    </script>\n            <script>\n        $(function () {\n            var Profile = {\n                $profile: $('#profile'),\n                $btnUpdate: $('#btn-update'),\n                originalUpdateButtonText: undefined,\n                isSyncing: false,\n                setIsSyncing: function (bool) {\n                    Profile.isSyncing = bool;\n\n                    if (bool === true) {\n                        Profile.originalUpdateButtonText = Profile.$btnUpdate.find('.status').text();\n                        Profile.$btnUpdate.find('.status').text('Updating');\n                        Profile.$btnUpdate.addClass('updating').removeClass('error').removeClass('fresh');\n                    } else {\n                        Profile.$btnUpdate.removeClass('updating').removeClass('error');\n                        Profile.$btnUpdate.find('.status').text(Profile.originalUpdateButtonText);\n                    }\n                },\n                requestSync: function () {\n                    var region = Profile.$profile.data('region');\n                    var name = Profile.$profile.data('name');\n\n                    axios\n                        .get('/profile/' + region + '/' + encodeURI(name) + '/sync')\n                        .then(function (response) {\n                            var json = response.data;\n\n                            if (json.error) {\n                                Profile.setIsSyncing(false);\n                                Profile.$btnUpdate.addClass('error').find('.status').text(json.message);\n                